# Feature Flags  
REACT_APP_ENABLE_IMAGE_RECOGNITION=true  
REACT_APP_ENABLE_ANALYTICS=false  
REACT_APP_ENABLE_USER_ACCOUNTS=false  

# Development  
REACT_APP_DEBUG_MODE=false  
REACT_APP_LOG_LEVEL="warn"  

# External Services  
REACT_APP_SPOONACULAR_API_KEY="your_spoonacular_key"  
REACT_APP_EDAMAM_APP_ID="your_edamam_app_id"  
REACT_APP_EDAMAM_APP_KEY="your_edamam_key"  